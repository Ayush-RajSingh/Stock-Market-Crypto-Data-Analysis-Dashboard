   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Data\n",
    "data = {'Category': ['A', 'B', 'C'], 'Values': [10, 20, 30]} \n",
    "df = pd.DataFrame(data) \n",
    "\n",
    "# Plotting\n",
    "plt.bar(df['Category'], df['Values'])\n",
    "plt.xlabel('Category')\n",
    "plt.ylabel('Values')\n",
    "plt.title('Sample Bar Chart')\n",
    "plt.show()\n"
   ]
  }
